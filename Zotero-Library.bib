@article{garoneMitochondrialDNATranscription2018,
  title = {Mitochondrial {{DNA}} Transcription and Translation: Clinical Syndromes},
  shorttitle = {Mitochondrial {{DNA}} Transcription and Translation},
  author = {Garone, Caterina and Minczuk, Michal and Boczonadi, Veronika and Ricci, Giulia and Horvath, Rita},
  date = {2018-07-20},
  journaltitle = {Essays in Biochemistry},
  shortjournal = {Essays Biochem},
  volume = {62},
  number = {3},
  pages = {321--340},
  publisher = {{Portland Press; https://web.archive.org/web/20220310220042/https://portlandpress.com/essaysbiochem/article/62/3/321/78584/Mitochondrial-DNA-transcription-and-translation}},
  issn = {0071-1365},
  doi = {10.1042/EBC20170103},
  url = {https://portlandpress.com/essaysbiochem/article/62/3/321/78584/Mitochondrial-DNA-transcription-and-translation},
  urldate = {2022-03-10},
  abstract = {Diagnosing primary mitochondrial diseases is challenging in clinical practice. Although, defective oxidative phosphorylation (OXPHOS) is the common final pathway, it is unknown why different mtDNA or nuclear mutations result in largely heterogeneous and often tissue -specific clinical presentations. Mitochondrial tRNA (mt-tRNA) mutations are frequent causes of mitochondrial diseases both in children and adults. However numerous nuclear mutations involved in mitochondrial protein synthesis affecting ubiquitously expressed genes have been reported in association with very tissue specific clinical manifestations suggesting that there are so far unknown factors determining the tissue specificity in mitochondrial translation. Most of these gene defects result in histological abnormalities and multiple respiratory chain defects in the affected organs. The clinical phenotypes are usually early-onset, severe, and often fatal, implying the importance of mitochondrial translation from birth. However, some rare, reversible infantile mitochondrial diseases are caused by very specific defects of mitochondrial translation. An unbiased genetic approach (whole exome sequencing, RNA sequencing) combined with proteomics and functional studies revealed novel factors involved in mitochondrial translation which contribute to the clinical manifestation and recovery in these rare reversible mitochondrial conditions.},
  langid = {english},
  keywords = {archived},
  note = {Version URL: {$<$}a href="https://web.archive.org/web/20220310220042/https://portlandpress.com/essaysbiochem/article/62/3/321/78584/Mitochondrial-DNA-transcription-and-translation" data-originalurl="https://portlandpress.com/essaysbiochem/article/62/3/321/78584/Mitochondrial-DNA-transcription-and-translation" data-versiondate="2022-03-10T22:00:42Z"{$>$}Robust Link for: https://portlandpress.com/essaysbiochem/article/62/3/321/78584/Mitochondrial-DNA-transcription-and-translation{$<$}/a{$>$}},
  file = {/Users/vannijnatten/Library/Mobile Documents/com~apple~CloudDocs/Zotero/storage/Q4HL3YPD/Garone et al_2018_Mitochondrial DNA transcription and translation.pdf;/Users/vannijnatten/Library/Mobile Documents/com~apple~CloudDocs/Zotero/storage/AHBBPYMQ/Mitochondrial-DNA-transcription-and-translation.html}
}

