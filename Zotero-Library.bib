@article{ongCurrentSmokingAssociated2019,
  title = {Current {{Smoking}} Is {{Associated}} with {{Decreased Expression}} of {{miR-335-5p}} in {{Parenchymal Lung Fibroblasts}}},
  author = {Ong, Jennie and van den Berg, Anke and Faiz, Alen and Boudewijn, Ilse and Timens, Wim and Vermeulen, Cornelis and Oliver, Brian and Kok, Klaas and Terpstra, Martijn and van den Berge, Maarten and Brandsma, Corry-Anke and Kluiver, Joost},
  options = {useprefix=true},
  date = {2019-10-18},
  journaltitle = {International Journal of Molecular Sciences},
  shortjournal = {IJMS},
  volume = {20},
  number = {20},
  pages = {5176},
  issn = {1422-0067},
  doi = {10.3390/ijms20205176},
  url = {https://www.mdpi.com/1422-0067/20/20/5176},
  urldate = {2022-03-11},
  abstract = {Cigarette smoking causes lung inflammation and tissue damage. Lung fibroblasts play a major role in tissue repair. Previous studies have reported smoking-associated changes in fibroblast responses and methylation patterns. Our aim was to identify the effect of current smoking on miRNA expression in primary lung fibroblasts. Small RNA sequencing was performed on lung fibroblasts from nine current and six ex-smokers with normal lung function. MiR-335-5p and miR-335-3p were significantly downregulated in lung fibroblasts from current compared to ex-smokers (false discovery rate (FDR) {$<$}0.05). Differential miR-335-5p expression was validated with RT-qPCR (p-value = 0.01). The results were validated in lung tissue from current and ex-smokers and in bronchial biopsies from non-diseased smokers and never-smokers (p-value {$<$}0.05). The methylation pattern of the miR-335 host gene, determined by methylation-specific qPCR, did not differ between current and ex-smokers. To obtain insights into the genes regulated by miR-335-5p in fibroblasts, we overlapped all proven miR-335-5p targets with our previously published miRNA targetome data in lung fibroblasts. This revealed Rb1, CARF, and SGK3 as likely targets of miR-335-5p in lung fibroblasts. Our study indicates that miR-335-5p downregulation due to current smoking may affect its function in lung fibroblasts by targeting Rb1, CARF and SGK3.},
  langid = {english},
  keywords = {archived},
  annotation = {https://web.archive.org/web/20220311215829/https://www.mdpi.com/1422-0067/20/20/5176},
  note = {Version URL: {$<$}a href="https://web.archive.org/web/20220311215829/https://www.mdpi.com/1422-0067/20/20/5176" data-originalurl="https://www.mdpi.com/1422-0067/20/20/5176" data-versiondate="2022-03-11T21:58:29Z"{$>$}Robust Link for: https://www.mdpi.com/1422-0067/20/20/5176{$<$}/a{$>$}},
  file = {/Users/vannijnatten/Library/Mobile Documents/com~apple~CloudDocs/Zotero/storage/MKYNXQYN/Ong et al_2019_Current Smoking is Associated with Decreased Expression of miR-335-5p in.pdf}
}

@article{orourkeWhatNormalizationStrategies2019,
  title = {What Is {{Normalization}}? {{The Strategies Employed}} in {{Top-Down}} and {{Bottom-Up Proteome Analysis Workflows}}},
  shorttitle = {What Is {{Normalization}}?},
  author = {O’Rourke, Matthew B. and Town, Stephanie E. L. and Dalla, Penelope V. and Bicknell, Fiona and Koh Belic, Naomi and Violi, Jake P. and Steele, Joel R. and Padula, Matthew P.},
  date = {2019-08-22},
  journaltitle = {Proteomes},
  shortjournal = {Proteomes},
  volume = {7},
  number = {3},
  pages = {29},
  issn = {2227-7382},
  doi = {10.3390/proteomes7030029},
  url = {https://www.mdpi.com/2227-7382/7/3/29},
  urldate = {2022-03-11},
  abstract = {The accurate quantification of changes in the abundance of proteins is one of the main applications of proteomics. The maintenance of accuracy can be affected by bias and error that can occur at many points in the experimental process, and normalization strategies are crucial to attempt to overcome this bias and return the sample to its regular biological condition, or normal state. Much work has been published on performing normalization on data post-acquisition with many algorithms and statistical processes available. However, there are many other sources of bias that can occur during experimental design and sample handling that are currently unaddressed. This article aims to cast light on the potential sources of bias and where normalization could be applied to return the sample to its normal state. Throughout we suggest solutions where possible but, in some cases, solutions are not available. Thus, we see this article as a starting point for discussion of the definition of and the issues surrounding the concept of normalization as it applies to the proteomic analysis of biological samples. Specifically, we discuss a wide range of different normalization techniques that can occur at each stage of the sample preparation and analysis process.},
  langid = {english},
  keywords = {archived,REVIEW},
  annotation = {https://web.archive.org/web/20220311210215/https://www.mdpi.com/2227-7382/7/3/29},
  note = {Version URL: {$<$}a href="https://web.archive.org/web/20220311210215/https://www.mdpi.com/2227-7382/7/3/29" data-originalurl="https://www.mdpi.com/2227-7382/7/3/29" data-versiondate="2022-03-11T21:02:15Z"{$>$}Robust Link for: https://www.mdpi.com/2227-7382/7/3/29{$<$}/a{$>$}},
  file = {/Users/vannijnatten/Library/Mobile Documents/com~apple~CloudDocs/Zotero/storage/Z4BRC8IP/O’Rourke et al_2019_What is Normalization.pdf}
}

@article{oseiUnravellingComplexityCOPD2015,
  title = {Unravelling the Complexity of {{COPD}} by {{microRNAs}}: It's a Small World after All},
  shorttitle = {Unravelling the Complexity of {{COPD}} by {{microRNAs}}},
  author = {Osei, Emmanuel T. and Florez-Sampedro, Laura and Timens, Wim and Postma, Dirkje S. and Heijink, Irene H. and Brandsma, Corry-Anke},
  date = {2015-09},
  journaltitle = {European Respiratory Journal},
  shortjournal = {Eur Respir J},
  volume = {46},
  number = {3},
  pages = {807--818},
  issn = {0903-1936, 1399-3003},
  doi = {10.1183/13993003.02139-2014},
  url = {http://erj.ersjournals.com/lookup/doi/10.1183/13993003.02139-2014},
  urldate = {2022-03-12},
  abstract = {Chronic obstructive pulmonary disease (COPD) is a progressive lung disease and is currently the fourth leading cause of death worldwide. Chronic inflammation and repair processes in the small airways are characteristic of COPD. Despite extensive efforts from researchers and industry, there is still no cure for COPD, hence an urgent need for new therapeutic alternatives. MicroRNAs are such an option; they are small noncoding RNAs involved in gene regulation. Their importance has been shown with respect to maintaining the balance between health and disease. Although previous reviews have discussed the expression of microRNAs related to lung disease, a detailed discussion regarding the function of differential miRNA expression in the pathogenesis of COPD is lacking.             In this review we link the expression of microRNAs to different features of COPD and explain their importance in the pathogenesis of this disease. We further discuss their potential to contribute to the development of future therapeutic strategies.},
  langid = {english},
  keywords = {archived},
  annotation = {https://web.archive.org/web/20220312001052/https://erj.ersjournals.com/content/46/3/807},
  note = {Version URL: {$<$}a href="https://web.archive.org/web/20220312001052/https://erj.ersjournals.com/content/46/3/807" data-originalurl="http://erj.ersjournals.com/lookup/doi/10.1183/13993003.02139-2014" data-versiondate="2022-03-12T00:10:52Z"{$>$}Robust Link for: http://erj.ersjournals.com/lookup/doi/10.1183/13993003.02139-2014{$<$}/a{$>$}},
  file = {/Users/vannijnatten/Library/Mobile Documents/com~apple~CloudDocs/Zotero/storage/XHW2LF5K/Osei et al_2015_Unravelling the complexity of COPD by microRNAs.pdf}
}

