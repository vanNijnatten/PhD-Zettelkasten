  note = {Version URL: {$<$}a href="https://web.archive.org/web/20220312001958/https://www.hindawi.com/journals/omcl/2013/746432/" data-originalurl="http://www.hindawi.com/journals/omcl/2013/746432/" data-versiondate="2022-03-12T00:19:58Z"{$>$}Robust Link for: http://www.hindawi.com/journals/omcl/2013/746432/{$<$}/a{$>$}},
  file = {/Users/vannijnatten/Library/Mobile Documents/com~apple~CloudDocs/Zotero/storage/SCSP8AQH/Bauer et al_2013_The Involvement of iNRF2-i in Lung Cancer.pdf}
