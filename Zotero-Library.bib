  file = {/Users/vannijnatten/Library/Mobile Documents/com~apple~CloudDocs/Zotero/storage/2TETHNV6/TS5-13059_2016_1066_MOESM5_ESM.csv;/Users/vannijnatten/Library/Mobile Documents/com~apple~CloudDocs/Zotero/storage/9EXGND5Y/TS8-13059_2016_1066_MOESM8_ESM.xls;/Users/vannijnatten/Library/Mobile Documents/com~apple~CloudDocs/Zotero/storage/AGC5V4PD/TS1-13059_2016_1066_MOESM1_ESM.csv;/Users/vannijnatten/Library/Mobile Documents/com~apple~CloudDocs/Zotero/storage/F9LBX2CV/Pidsley et al_2016_Critical evaluation of the Illumina MethylationEPIC BeadChip microarray for.pdf;/Users/vannijnatten/Library/Mobile Documents/com~apple~CloudDocs/Zotero/storage/FYU8LJVA/TS2-13059_2016_1066_MOESM2_ESM.csv;/Users/vannijnatten/Library/Mobile Documents/com~apple~CloudDocs/Zotero/storage/H32UY729/TS6-13059_2016_1066_MOESM6_ESM.csv;/Users/vannijnatten/Library/Mobile Documents/com~apple~CloudDocs/Zotero/storage/JGUAPJA8/TS7-13059_2016_1066_MOESM7_ESM.pdf;/Users/vannijnatten/Library/Mobile Documents/com~apple~CloudDocs/Zotero/storage/XGN6ATT5/TS4-13059_2016_1066_MOESM4_ESM.csv;/Users/vannijnatten/Library/Mobile Documents/com~apple~CloudDocs/Zotero/storage/ZDBZNZZ9/TS3-13059_2016_1066_MOESM3_ESM.csv;/Users/vannijnatten/Library/Mobile Documents/com~apple~CloudDocs/Zotero/storage/IFX63WGA/s13059-016-1066-1.html}
