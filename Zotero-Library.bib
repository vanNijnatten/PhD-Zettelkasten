@report{sudermanDmrffIdentifyingDifferentially2018,
  type = {preprint},
  title = {Dmrff: Identifying Differentially Methylated Regions Efficiently with Power and Control},
  shorttitle = {Dmrff},
  author = {Suderman, Matthew and Staley, James R and French, Robert and Arathimos, Ryan and Simpkin, Andrew and Tilling, Kate},
  date = {2018-12-31},
  institution = {{Bioinformatics}},
  doi = {10.1101/508556; https://web.archive.org/web/20220311213841/https://www.biorxiv.org/content/10.1101/508556v1},
  url = {http://biorxiv.org/lookup/doi/10.1101/508556},
  urldate = {2022-03-11},
  abstract = {A differentially methylated region (DMR) is a genomic region in which DNA methylation is consistently positively or negatively associated with a phenotype or exposure. We demonstrate that existing algorithms for identifying DMRs either fail to consistently control false positive rates (comb-p and DMRcate), suffer from low power (bumphunter) or lack modeling flexibility (seqlm). We introduce a new method, dmrff, that overcomes these shortcomings and can additionally be used to meta-analyze multiple datasets. When applied to investigate associations of age in multiple datasets, dmrff identifies novel DMRs near genes previously linked to age. An R implementation is available on Github (http://github.com/perishky/dmrff).},
  langid = {english},
  keywords = {archived},
  note = {Version URL: {$<$}a href="https://web.archive.org/web/20220311213841/https://www.biorxiv.org/content/10.1101/508556v1" data-originalurl="http://biorxiv.org/lookup/doi/10.1101/508556" data-versiondate="2022-03-11T21:38:41Z"{$>$}Robust Link for: http://biorxiv.org/lookup/doi/10.1101/508556{$<$}/a{$>$}},
  file = {/Users/vannijnatten/Library/Mobile Documents/com~apple~CloudDocs/Zotero/storage/FEG6I86F/Suderman et al_2018_dmrff.pdf}
}

