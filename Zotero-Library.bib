  shorttitle = {Translation},
  author = {Clancy, Suzanne and Brown, William},
  url = {https://www.nature.com/scitable/topicpage/translation-dna-to-mrna-to-protein-393/},
  urldate = {2022-03-10},
  abstract = {Genes encode proteins, and the instructions for making proteins are decoded in two steps: first, a messenger RNA (mRNA) molecule is produced through the transcription of DNA, and next, the mRNA serves as a template for protein production through the process of translation. The mRNA specifies, in triplet code, the amino acid sequence of proteins; the code is then read by transfer RNA (tRNA) molecules in a cell structure called the ribosome. The genetic code is identical in prokaryotes and eukaryotes, and the process of translation is very similar, underscoring its vital importance to the life of the cell.},
  langid = {english},
  keywords = {archived},
  annotation = {Cg\_cat: Translation: DNA to mRNA to Protein Cg\_level: ESY Cg\_topic: Translation: DNA to mRNA to Protein; https://web.archive.org/web/20220310223922/https://www.nature.com/scitable/topicpage/translation-dna-to-mrna-to-protein-393/},
