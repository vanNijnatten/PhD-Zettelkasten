@article{schuursEffectsGenderSex1990,
  title = {Effects of Gender and Sex Steroids on the Immune Response},
  author = {Schuurs, A. H. W. M. and Verheul, H. A. M.},
  date = {1990-02-01},
  journaltitle = {Journal of Steroid Biochemistry},
  shortjournal = {Journal of Steroid Biochemistry},
  volume = {35},
  number = {2},
  pages = {157--172},
  issn = {0022-4731},
  doi = {10/d8qf9h},
  url = {https://www.sciencedirect.com/science/article/pii/0022473190902703},
  urldate = {2021-09-04},
  abstract = {Elevated immune responses and the higher incidence of autoimmune diseases in female (compared to male) humans and animals have been known for a long time. However, the scientific interest in this interrelationship has been limited both amongst immunologists and endocrinologists. It is mainly in the last ten years that investigations in this area have been intensifying. A number of fairly recent review articles confirm the increased interest in various aspects of this “interdiscipline” [1–4]. In the present paper we should like to make a new assessment of the state of knowledge. We shall firstly discuss heteroimmune response differences between males and females in humans, rodents and birds and then the roles of gender and sex hormones in autoimmune disease in various species. The general conclusions are the following. Gender and sex hormones have a clear effect on various hetero- and auto-immune responses but the mechanisms of action are still unknown; starting from sex hormones, steroids can be devised which have favourable effects on immune processes but lack undesirable hormonal effects; such hormonomimetics should be, in principle, applicable for the treatment of autoimmune disease.},
  langid = {english},
  keywords = {archived,REVIEW},
  annotation = {https://web.archive.org/web/20210904072357/https://www.sciencedirect.com/science/article/abs/pii/0022473190902703},
  note = {Version URL: {$<$}a href="https://web.archive.org/web/20210904072357/https://www.sciencedirect.com/science/article/abs/pii/0022473190902703" data-originalurl="https://www.sciencedirect.com/science/article/pii/0022473190902703" data-versiondate="2021-09-04T07:23:57Z"{$>$}Robust Link for: https://www.sciencedirect.com/science/article/pii/0022473190902703{$<$}/a{$>$}},
  file = {/Users/vannijnatten/Library/Mobile Documents/com~apple~CloudDocs/Zotero/storage/5J4GAGRX/Schuurs_Verheul_1990_Effects of gender and sex steroids on the immune response.pdf;/Users/vannijnatten/Library/Mobile Documents/com~apple~CloudDocs/Zotero/storage/GHL6DRCH/0022473190902703.html}
